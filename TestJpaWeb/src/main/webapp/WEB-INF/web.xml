		<servlet-class>com.example.jpa.web.identitygenerators.TableIdentityGeneratorTestServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>IdentityGeneratorTestServlet</display-name>
		<servlet-name>IdentityGeneratorTestServlet</servlet-name>
		<servlet-class>com.example.jpa.web.identitygenerators.IdentityGeneratorTestServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>AutoIdentityGeneratorTestServlet</display-name>
		<servlet-name>AutoIdentityGeneratorTestServlet</servlet-name>
		<servlet-class>com.example.jpa.web.identitygenerators.AutoIdentityGeneratorTestServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>OrderedListTestServlet</display-name>
		<servlet-name>OrderedListTestServlet</servlet-name>
		<servlet-class>com.example.jpa.web.collectionsorting.OrderedListTestServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>UserRestHandler</display-name>
		<servlet-name>UserRestHandler</servlet-name>
		<servlet-class>com.example.jpa.web.collectionsorting.UserRestHandler</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
