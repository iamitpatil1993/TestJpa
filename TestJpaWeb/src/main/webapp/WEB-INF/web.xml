	<servlet-mapping>
		<servlet-name>UserRegistrationServlet</servlet-name>
		<url-pattern>/userRegister</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>EnumMappingTestServlet</servlet-name>
		<url-pattern>/EnumMappingTestServlet</url-pattern>
